<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nz2dev.tenantcloudgoods">

    <application
        android:name=".app.TenantCloudGoodsApp"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".app.presentation.modules.auth.AuthorizationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.presentation.modules.home.HomeActivity" />
        <activity android:name=".app.presentation.modules.shop.ShopActivity" />
        <activity android:name=".app.presentation.modules.history.PaymentHistoryActivity" />
    </application>
</manifest>
